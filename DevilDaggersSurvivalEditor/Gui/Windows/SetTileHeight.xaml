<Window x:Class="DevilDaggersSurvivalEditor.Gui.Windows.SetTileHeightWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Set tile height" Height="256" Width="256" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded">
	<StackPanel x:Name="Data" Style="{StaticResource PaddingStackPanel}">
		<Label Style="{StaticResource BoldLabel}" Name="CoordinatesLabel" />
		<TextBox Name="TileHeightTextBox" Text="{Binding TileHeight}" />
		<Button Style="{StaticResource OkButton}" Name="OkButton" Click="OkButton_Click">OK</Button>
	</StackPanel>
</Window>