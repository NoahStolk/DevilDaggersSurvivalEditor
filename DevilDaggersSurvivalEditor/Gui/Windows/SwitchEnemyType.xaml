<Window x:Class="DevilDaggersSurvivalEditor.Gui.Windows.SwitchEnemyTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Switch enemy type" Height="512" Width="512" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
	<StackPanel Style="{StaticResource PaddingStackPanel}">
		<Label Style="{StaticResource BoldLabel}" Name="SpawnsLabel" />
		<StackPanel Name="SwitchStackPanel" />
		<Button Style="{StaticResource OkButton}" Name="OkButton" Click="OkButton_Click">OK</Button>
	</StackPanel>
</Window>