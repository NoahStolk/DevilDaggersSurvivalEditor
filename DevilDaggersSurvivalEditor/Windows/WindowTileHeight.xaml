<Window x:Class="DevilDaggersSurvivalEditor.Windows.WindowTileHeight"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Set tile height" Height="200" Width="200" ResizeMode="NoResize" Background="Black">
	<Grid>
		<StackPanel>
			<TextBox Margin="4,4,4,4" Name="TileHeightTextBox"></TextBox>
			<Button Margin="4,4,4,4" Name="OKButton" Content="OK" Click="OKButton_Click"/>
		</StackPanel>
	</Grid>
</Window>