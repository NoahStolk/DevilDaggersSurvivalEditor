<Window x:Class="DevilDaggersSurvivalEditor.GUI.Windows.ArenaPresetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Height="400" Width="400" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
	<Grid Margin="10">
		<StackPanel>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition />
					<ColumnDefinition />
				</Grid.ColumnDefinitions>
				<StackPanel Grid.Column="0" Name="OptionLabels" />
				<StackPanel Grid.Column="1" Name="OptionInputs" />
			</Grid>
			<StackPanel>
				<Button Click="GenerateButton_Click">Generate</Button>
			</StackPanel>
		</StackPanel>
	</Grid>
</Window>