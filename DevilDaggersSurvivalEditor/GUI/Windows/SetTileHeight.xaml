<Window x:Class="DevilDaggersSurvivalEditor.GUI.Windows.SetTileHeightWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Set tile height" Height="256" Width="256" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
	<Grid>
		<StackPanel x:Name="Data">
			<TextBlock Name="CoordinatesLabel" />
			<TextBox Name="TileHeightTextBox" Text="{Binding TileHeight}" />
			<Button Name="OKButton" Content="OK" Click="OKButton_Click" />
		</StackPanel>
	</Grid>
</Window>