<Window x:Class="DevilDaggersSurvivalEditor.GUI.Windows.SetTileHeightWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Set tile height" Height="200" Width="200" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
	<Grid Margin="10">
		<StackPanel>
			<TextBox Name="TileHeightTextBox"></TextBox>
			<Button Name="OKButton" Content="OK" Click="OKButton_Click"/>
		</StackPanel>
	</Grid>
</Window>