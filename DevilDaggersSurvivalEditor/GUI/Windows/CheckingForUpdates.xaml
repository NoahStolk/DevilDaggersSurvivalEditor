<Window x:Class="DevilDaggersSurvivalEditor.GUI.Windows.CheckingForUpdatesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Checking for updates" Height="200" Width="400" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
	<StackPanel Style="{StaticResource PaddingStackPanel}">
		<TextBlock HorizontalAlignment="Center">Connecting...</TextBlock>
		<Button Style="{StaticResource OKButton}" Name="CancelButton" Click="CancelButton_Click">Cancel</Button>
	</StackPanel>
</Window>